double d1=Math.random();        //生成一个double型,范围[0.0,1.0)左闭右开.
math.round();                   //加上0.5之后向下取整。
a.equals(b);                    //比较对象内容是否相同,而==比较地址是否相同.

//switch括号的数据类型:byte,short,int,char,枚举,String.
switch(a){
    default:
	      System.out.println("fault");  //如果default放前面或者中间而且不加break,则会继续往下运行.
    case(0):
	      System.out.println("0");
	      break;
}

a:for(){
    for(){
         break a;      //结束指定循环。
         continue a;   //结束指定循环的当次循环。
    }
}
